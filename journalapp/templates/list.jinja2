{% extends "base.jinja2" %}

{% block content %}

    <p>List of Learning Journal Entries goes here</p>

    {% for entry in entries %}

        <h4><a href="/detail/{{entry.id}}">{{entry.title}}</a></h4>
        <p>{{entry.created.strftime('%Y-%m-%d at %H:%M UTC')}}</p>

    {% endfor %}

{% endblock %}